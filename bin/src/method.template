
    /**
     * Get or set the {$text_method_name}.
     *
     * @param mixed {$method_argument}
     */
    public function {$new_method_name}()
    {
        if (func_num_args() == 0) {
            return $this->get{$method_name}();
        }

        $this->set{$method_name}(func_get_arg(0));

        return $this;
    }
