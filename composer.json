{
    "name": "bluora/laravel-slack-api",
    "description": "Laravel integration for the cleentfaar/slack package.",
    "keywords": ["slack", "laravel", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@bluora.com.au"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "bluora/php-slack-api": "0.20.13"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "squizlabs/php_codesniffer": "^2.0"
    },    
    "autoload": {
        "psr-4": {
            "Bluora\\LaravelSlackApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bluora\\LaravelSlackApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "composer phpcs",
        "phpcs": "./vendor/bin/phpcs -p -s --standard=PSR2 ./src"
    }
}
